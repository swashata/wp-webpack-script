{
  "name": "@wpackio/eslint-config",
  "version": "6.3.0",
  "description": "Shared ESLint config for all @wpackio packages.",
  "keywords": [
    "eslint",
    "typescript",
    "eslint-config",
    "eslint-typescript-config"
  ],
  "main": "index.js",
